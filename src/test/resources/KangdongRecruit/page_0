	<script type="text/javascript">
		var strInggbn	= "1";

		$(document).ready(function() {
			var event;
			if (document.createEvent) {
				event = document.createEvent("HTMLEvents");
				event.initEvent("click", true, false);
			} else {
				event = document.createEventObject();
				event.eventType = "click";
			}

			//var event = document.createEvent("HTMLEvents");
			//event.initEvent("click", true, false);
			//document.getElementById('liRecTap'+strInggbn).dispatchEvent(event);
		});

		function open_guide_pop_close() {
			$("#guidePop").hide();
		}

		function fn_pop_guide() {
			if ( mobile_chk == "M" ) {
				alert('PC 웹사이트에서 이용해 주십시오.');
				return false;
			} else {
				var url, w, h
				url = "/hrt_pop_guide.jsp";
				w	= "999";
				h	= "780";
				window.open(url,"_blank","scrollbars=no,toolbar=no,location=no,directories=no,width="+w+",Height="+h);
			}
		}

		function fn_reissuance(url) {
			if ( mobile_chk == "M" ) {
				alert('PC 웹사이트에서 이용해 주십시오.');
				return false;
			} else {
				var chkIeVer = $("#chkBrowser").val();

				if ( chkIeVer == "less" ) {
					location.href = "https://recruit.kdh.or.kr:446/"+url;
				} else {
					location.href = url;
				}
			}
		}

		var filter = "win16|win32|win64|mac|macintel";
		var mobile_chk = "";

		if ( navigator.platform ) {
			if ( filter.indexOf( navigator.platform.toLowerCase() ) < 0 ) {
				//mobile
				mobile_chk = "M";
			} else {
				//pc
				mobile_chk = "P";
			}
		}

		function fn_GoUrl1(url) {
			if ( mobile_chk == "M" ) {
				alert('온라인 입사지원은 PC 웹사이트에서 이용해 주십시오.');
				return false;
			} else {
				var chkIeVer = $("#chkBrowser").val();

				if ( chkIeVer == "less" ) {
					location.href = "https://recruit.kdh.or.kr:446/"+url;
				} else {
					location.href = url;
				}
			}
		}

		function fn_GoUrl2(url) {
			var chkIeVer = $("#chkBrowser").val();

			if ( chkIeVer == "less" ) {
				location.href = "https://recruit.kdh.or.kr:446/"+url;
			} else {
				location.href = url;
			}
		}

		var gallery_num = 0;/*이미지 출력번호 기록*/
		var btn_num = 0;

		$(function(){
			var $source	= $(".visual_in li img.source-image").attr("src");
			var img_h		=  parseInt( $(".visual_in li img").css("height") )

			//$(".visual_in li img").css({backgroundImage:"url("+$source+")", backgroundRepeat:"no-repeat", backgroundPosition:"center 0"})
			$(".source-image").css({height:img_h})
			$(".visual_btn span:first").css({background:"#f27186",borderRauids:10}).animate({width:20});
			$(".visual_btn span").click(function(){
				$(".visual_in").animate({marginLeft: -parseInt($(".visual_in li").css("width"))*$(this).index()});
				btn_num = gallery_num =  $(this).index();
				fntest(btn_num);
				$(".visual_in").css({height:img_h})
			});
		});

		function fntest(num){
			 $(".visual_btn span").css({background:"#666"}).animate({width:10});
			 $(".visual_btn span:eq("+num+")").css({background:"#f27186",borderRauids:10}).animate({width:30});
		}

		function fn_chgTab(val){
			if (val=="ing") {
				location.href="index.jsp?inggbn=ing&movePage=1";
			} else if (val=="end") {
				location.href="index.jsp?inggbn=end&movePage=1";
			} else if (val=="all") {
				location.href="index.jsp?inggbn=all&movePage=1";
			}
		}

		function get_ver_IE_head() {
			var word, version = "N/A", agent = navigator.userAgent.toLowerCase();
			/*IE old version ( IE 10 or Lower ) */
			if ( navigator.appName == "Microsoft Internet Explorer" ) word = "msie ";
			else {
				/*IE 11 */
				if ( agent.search("trident") > -1 ) word = "trident/.*rv:";
				/*Edge  */
				else if ( agent.search("edge/") > -1 ) word = "edge/";
			}

			var reg = new RegExp( word + "([0-9]{1,})(\\.{0,}[0-9]{0,1})" );
			if (  reg.exec( agent ) != null  ) version = RegExp.$1 + RegExp.$2;
			return parseInt(version, 10);
		}

		var is_ie_head = (navigator.appName=="Microsoft Internet Explorer")
		var chkBrowser = "";

		if (is_ie_head) {
			var ver_head = get_ver_IE_head();
			if (ver_head <= 9) {
				chkBrowser = "less";
			} else {
				chkBrowser = "exceed";
			}
		} else {
			chkBrowser = "other";
		}

		$(document).ready(function() {
			$("#chkBrowser").val(chkBrowser);
		});
	</script>
